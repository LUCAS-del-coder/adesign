# 數據庫配置
DATABASE_URL=mysql://user:password@localhost:3306/adesign
# 或使用 MYSQL_URL（Railway 默認）
# MYSQL_URL=mysql://user:password@localhost:3306/adesign

# JWT 密鑰（用於會話管理）
JWT_SECRET=your-secret-key-here-change-this-to-a-random-string

# Google OAuth 配置
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:5173/api/oauth/callback

# 前端 Google OAuth（可選，用於構建時）
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_GOOGLE_REDIRECT_URI=http://localhost:5173/api/oauth/callback

# Cloudflare R2 配置
CLOUDFLARE_ACCOUNT_ID=your-account-id
CLOUDFLARE_R2_BUCKET=your-bucket-name
CLOUDFLARE_R2_ACCESS_KEY_ID=your-access-key-id
CLOUDFLARE_R2_SECRET_ACCESS_KEY=your-secret-access-key
# 可選：R2 公開 URL（如果設置了公開域名）
# CLOUDFLARE_R2_PUBLIC_URL=https://your-bucket.r2.dev

# Gemini API 配置
GEMINI_API_KEY=your-gemini-api-key
# 可選：指定圖片生成模型
# GEMINI_IMAGE_MODEL=gemini-3-pro-image-preview

# 應用配置
NODE_ENV=development
PORT=5173
